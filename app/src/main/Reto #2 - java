public class reto_10 {
   

    public static void main(String[] args) {
        int n = 50;
        Long[] fibonacci = new Long[(int )n];

    fibonacci[0] = 0L;
    fibonacci[1] = 1L;

System.out.println("Los primeros " + n + " n√∫meros de la secuencia de Fibonacci son:");

    for (int i = 2; i < n; i++) {
    fibonacci[i] = fibonacci[i-1] + fibonacci[i-2];
    System.out.print(fibonacci[i] + " ");
    }

        
    
    
}
    
       
}
